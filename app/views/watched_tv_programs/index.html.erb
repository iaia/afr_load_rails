<p id="notice"><%= notice %></p>

<h1>Watched Tv Programs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @watched_tv_programs.each do |watched_tv| %>
      <tr>
          <td><%= watched_tv.tv_program.title_ja %></td>
          <td><%= watched_tv.tv_program.movie.director.name_ja %></td>
          <td><%= watched_tv.watched_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Watched Tv Program', new_watched_tv_program_path %>
