<p id="notice"><%= notice %></p>

<h1>Watched Tv Programs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @watched_tv_programs.each do |watched_tv_program| %>
      <tr>
        <td><%= link_to 'Show', watched_tv_program %></td>
        <td><%= link_to 'Edit', edit_watched_tv_program_path(watched_tv_program) %></td>
        <td><%= link_to 'Destroy', watched_tv_program, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Watched Tv Program', new_watched_tv_program_path %>
